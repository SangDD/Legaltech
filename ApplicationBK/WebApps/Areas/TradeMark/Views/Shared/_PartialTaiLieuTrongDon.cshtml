    <div class="div-search-title">
        <div>làm bằng tiếng </div>
    </div>
    <div class="div-search-content">
        <div style="width:150px">
            <select id="_6_cboNgonNguMauDangKy">
                @foreach (var item in MemoryData.c_lst_Country)
                {
                    if (item.Country_Id == Common.Common.Country_VietNam_Id)
                    {
                        <option selected="selected" value="@item.Country_Id">@item.Name</option>
                    }
                    else
                    {
                        <option value="@item.Country_Id">@item.Name</option>
                    }
                }
            </select>
        </div>
    </div>

    <div class="div-search-title">
        <div>số trang:</div>
    </div>
    <div class="div-search-content">
        <div>
            <input type="text" id="_6_txtnumberpage" maxlength=" 50" placeholder="_6_txtnumberpage" onkeypress="return isNumberKey(event);" onkeyup="jsFormatNumber(this.value, this.id)" />
        </div>
    </div>

    <div class="div-search-title">
        <div>số bản:</div>
    </div>
    <div class="div-search-content">
        <div>
            <input type="text" id="_6_txtnumberCopy" maxlength="50" placeholder="_6_txtnumberCopy" onkeypress="return isNumberKey(event);" onkeyup="jsFormatNumber(this.value, this.id)" />
        </div>
    </div>

