@using BussinessFacade.ModuleMemoryData
<div class="div-search-ad">
    <div class="div-search-content">
        <div class="div-checkbox" style="width:200px">
            <input type="checkbox" maxlength="3" value="C06DKQT_D_01" id="_6_rdoToKhai" onchange="return ShowHidenUpLoad(this.id, this.id);" />
            Tờ khai, gồm:
        </div>
    </div>
    <div class="div-search-title">
        <div>số trang: </div>
    </div>
    <div class="div-search-content">
        <div>
            <input style="width:140px" type="text" id="_6_txtToKhaiTrang" maxlength="50" placeholder="_6_txtToKhaiTrang" onkeypress="return isNumberKey(event);" onkeyup="jsFormatNumber(this.value, this.id)" />
        </div>

    </div>
    <div class="div-search-title">
        <div>
            số bản:
        </div>
    </div>
    <div class="div-search-content">
        <div>
            <input type="text" id="_6_txtToKhaiBan" maxlength="50" placeholder="_6_txtToKhaiBan" onkeypress="return isNumberKey(event);" onkeyup="jsFormatNumber(this.value, this.id)" />
        </div>
    </div>

</div>

<div class="div-search-ad">
    <div class="div-search-content">
        <div>
            @Html.Partial("~/Areas/TradeMark/Views/Shared/_PartialTaiLieuTrongDonFileInput.cshtml", "_6_rdoToKhai")
        </div>
    </div>
</div>

<div class="div-search-ad">
