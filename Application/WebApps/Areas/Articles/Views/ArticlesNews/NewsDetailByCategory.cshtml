@using ObjectInfos
@{
    var objInfo = new NewsInfo();
    if (ViewBag.objNewInfo != null)
    {
        objInfo = ViewBag.objNewInfo;


        ViewBag.Title = objInfo.Title;
    }
    Layout = "~/ViewsShared/_LayoutHomeArticles.cshtml";
}
<style>
    #divClose:hover {
        background-color: #da4347;
    }
</style>
<div class="t-generic_col-right o-layout_item" style="padding-top:100px;margin-left:20px">
    <div class="t-generic_col-right-wrap o-layout">
        <div class="c-content o-layout_item js-entry-container is-enabled is-entry-loaded" style="padding-left: 10px;">

            <h1 class="t-generic_title">@objInfo.Title</h1>
            <div class="t-generic_summary">
                @Html.Raw(objInfo.Content)
            </div>
        </div>
    </div>
</div>

@Html.Partial("~/ViewsShared/_PartialViewFaceYoutube.cshtml")

 