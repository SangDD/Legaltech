    $("#ckb_DOC_10").change(function () {
        funcShowOtherdocument();
    });
    function funcShowOtherdocument()
    {
        try {
            if ($("#ckb_DOC_10").is(":checked") == true) {
                $("#divContainerDocOther").css('display', 'block')
            } else {
                $("#divContainerDocOther").css('display', 'none');
            }
        } catch (e) {
            console.log(e);
        }
    }
    
</script>
