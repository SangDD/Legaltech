            formData.append("pAppDocumentInfo[" + pItem + "].Isuse", Isuse);
            return true;
        } catch (e) {
            return false;
        }

    }


    function funcGetTenTaiLieuKhac(formData) {
        try {
            var item = 0;
            $(".clssListUpload").each(function () {
                formData.append("pAppDocOtherInfo[" + item + "].keyFileUpload", this.id);
                formData.append("pAppDocOtherInfo[" + item + "].Documentname", $("#" + this.id).val());
                item++;
            });
            return true;
        } catch (e) {
            return false;
        }
    }


    //undo lai
    function UndoFileEdit(pID, pdivUp) {
        try {
            $("." + pID).css('display', 'block');

            var arrlistID = $("#txtDocumentInAppRemove").val();
            var pIDUndo = pID + "|";
            if (arrlistID.indexOf(pIDUndo) != -1) {
                arrlistID = arrlistID.replace(pIDUndo, "");
            }
            $("#Pre_" + pdivUp).removeClass("clssDisplayBlock");
            $("#Pre_" + pdivUp).addClass("clssDisplaynone");
            $("#txtDocumentInAppRemove").val(arrlistID);
        } catch (e) {
        }
