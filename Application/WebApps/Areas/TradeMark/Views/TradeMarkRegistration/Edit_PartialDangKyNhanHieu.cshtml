            $("." + pID).css('display', 'none');
            var lstID = $("#txtDocumentInAppRemove").val();
            lstID = lstID + pID + "|";
            $("#txtDocumentInAppRemove").val(lstID);
            $("#Pre_" + pdivUp).removeClass("clssDisplaynone");
            $("#Pre_" + pdivUp).addClass("clssDisplayBlock");
            //Cho ddown UT2
            $("#div_up_" + pdivUp).removeClass("clssDisplaynone");
            $("#div_up_" + pdivUp).addClass("clssDisplayBlock");

        } catch (e) {
        }
    }

    //undo lai
    function UndoFileEdit(pID, pdivUp) {
        try {
            $("." + pID).css('display', 'block');

            var arrlistID = $("#txtDocumentInAppRemove").val();
            var pIDUndo = pID + "|";
            if (arrlistID.indexOf(pIDUndo) != -1) {
                arrlistID = arrlistID.replace(pIDUndo, "");
            }
            $("#Pre_" + pdivUp).removeClass("clssDisplayBlock");
            $("#Pre_" + pdivUp).addClass("clssDisplaynone");
            $("#txtDocumentInAppRemove").val(arrlistID);
        } catch (e) {
        }
    }

