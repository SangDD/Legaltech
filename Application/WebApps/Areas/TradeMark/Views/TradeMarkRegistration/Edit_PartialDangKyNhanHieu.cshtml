    function funcSaveType(pStatus, pUpdateTodo) {
        try {
            //Thông tin chủ đơn
            var formData = new FormData();

            if (pUpdateTodo == 1) {
                formData.append("pInfo.UpdateToDo", 1);
            }

            var reponse = funcThongTinCaseName(formData, "0");
            if (reponse == false) {
                return false;
            }

            if (funcThongTinChuDonNguoiDaiDien(formData, "1") != true) {
                return false;
            }
            var v_AppCode = $("#txtAppCode").val();
            if (v_AppCode === "") {
                $('#txtAppCode').focus().val('');
                showError('Chưa chọn loại đơn đăng ký!');
                return false;
            }
            if (_checkanh === 1) {
                showError('Ảnh tải lên không đúng kích cỡ từ 2x2 đến 8x8!');
                return false;
            }
            if (_checkanh === 2) {
                showError('File tải lên không đúng định dạng file ảnh');
                return false;
            }
            formData.append("pInfo.Appcode", v_AppCode);

            var v_AppHeaderID = $("#txtAppHeaderID").val();
            if (v_AppHeaderID === "") {
                $('#txtAppHeaderID').focus().val('');
                showError('Không lấy được ID bản ghi!');
                return false;
            }
            formData.append("pInfo.Id", v_AppHeaderID);
            formData.append("pInfo.Case_Code", $("#txtCase_Code").val());

            var daidienSH = $("#txtDaiDienSHCN").val();
            //if (daidienSH === "") {
            //    $('#txtDaiDienSHCN').focus().val('');
            //    showError('Chưa chọn chủ đại đại diện sở hữu.');
            //    return false;
            //}
            formData.append("pInfo.DDSHCN", daidienSH);

            //07/09/2018
            var madhsh = $("#txtMADDSHCN").val();
            formData.append("pInfo.MADDSHCN", madhsh);


            if (funcThongTinChuDonNguoiDaiDien(formData, "2") != true) {
                return false;
            }
            var v_RepMasterType = $("input[name='3_rdoDaiDienChuDon']:checked").val();
            if (v_RepMasterType === "" || v_RepMasterType == undefined) {
                showError('Phải chọn ít nhất 1 giá trị đại điện cho chủ đơn!');
                return false;
            }
            formData.append("pInfo.Rep_Master_Type", v_RepMasterType);
            //trang thai
            formData.append("pInfo.Status", pStatus);
            //Lấy thông tin detail
            var reponse = funcGetAppDetail(formData);
            if (reponse == false) {
                return false;
            }
            //Lấy thông tin file có trong đơn
            reponse = funGetThongTinFile(formData);
            if (reponse == false) {
                return false;
            }
            reponse = funcGetDataClassInfo(formData);
            if (reponse == false) {
                return false;
            }
            //lay tai lieu khac
            reponse = funcGetTenTaiLieuKhac(formData);
            if (reponse == false) {
                return false;
            }
            //
            reponse = funcGetCamKetChuDon(formData);
            if (reponse == false) {
                return false;
            }
            $.ajax({
                url: '/trade-mark/dang-ky-nhan-hieu-sua-doi',
                type: 'POST',
                data: formData,
                processData: false, contentType: false, traditional: true, dataType: "json",
                enctype: 'multipart/form-data', async: false, headers: { "cache-control": "no-cache" },
                success: function (data) {
                    if (data != null && data.status >= 0) {
                        //nvsAlert(EditSuccess, ThongBao, function () {
                        //    window.location = "/trade-mark-mana/quan-ly-don";
                        //});

                        if (pStatus == 1) {
                            nvsAlert('@Html.Raw(Resource.LuuTamThanhCong)', ThongBao, function () {
                                window.location = "/trade-mark-mana/quan-ly-don";
                            });
                        } else if (pStatus == 2) {
                            nvsAlert('@Html.Raw(Resource.GuiDonThanhCong)', ThongBao, function () {
                                window.location = "/trade-mark-mana/quan-ly-don";
                            });
                        } else {
                            nvsAlert('@Html.Raw(Resource.SuaDonThanhCong)', ThongBao, function () {
                                window.location = "/trade-mark-mana/quan-ly-don";
                            });
                        }

                    } else {
                        nvsError(EditFail, ThongBao, function () {
                        });
                    }
                },
                error: function (e) {
                    console.log(e);
                }
            });

        } catch (e) {
        }
    }


