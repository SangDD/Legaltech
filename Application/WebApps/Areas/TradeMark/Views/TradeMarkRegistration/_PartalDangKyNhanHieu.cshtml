                </div>
            </div>
            <script>
                function funcChoiseDonUT(pID) {
                    try {
                        if (pID == "0_ckbCoSuDungDonUuTien") {
                            $('#0_ckbCoSuDungDonUuTien').prop('checked', true);
                            $('#divDonUuTienSo1').css('display', 'block');
                            $('#KOUT').prop('checked', false);
                            funcEnableDisable('classDacTinhUT', true);
                            $("#6_txtNuocNopDon").multipleSelect("enable")

                        } else {
                            $('#KOUT').prop('checked', true);
                            $('#0_ckbCoSuDungDonUuTien').prop('checked', false);
                            $('#divDonUuTienSo1').css('display', 'none');
                            funcEnableDisable('classDacTinhUT', false);
                            $("#6_txtNuocNopDon").multipleSelect("disable")
                            //Reset form 
                            $("#6_txtSoDon").val("")
                            $("#6_txtNgayNopDon").val("")
                            $('#4_ckbNopTaiVietNam').prop('checked', false);
                            $('#4_ckbNopTaiParis').prop('checked', false);
                            $('#4_ckbThoaThuanKhac').prop('checked', false);
                            $("#txtThoathuanKhac").val("")
                            

                            $('#6_rdoTLCMQUT').prop('checked', false);
                            $('#6_rdoBSDDT').prop('checked', false);
                            $('#6_rdoBDTVGom').prop('checked', false);
                            $('#6_rdoGiayChuyenNhungUT').prop('checked', false);

                            $("#6_txtBDTVGom").val("")
                            $("#6_txtBSDDT").val("")
                            $("#6_txtTLCMQUT").val("")
                        }
                    } catch (e) {
                    }
                }
            </script>

