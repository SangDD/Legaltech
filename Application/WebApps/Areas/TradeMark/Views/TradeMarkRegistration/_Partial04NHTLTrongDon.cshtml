        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoBanGocDaNopTheoDonSo")
    </div>

    @* bản dịch tiếng việt gồm *@
    <div class="div-search-ad" style="display:inline-flex;margin-left: 40px;">
        <div class="div-search-content">
            <div class="div-checkbox" style="width:801px">
                <input type="checkbox" id="6_rdoBanDichTiengViet" value="04NH_D_09" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                <label>@WebApps.Resources.Resource.lblBanDichTiengVietGom</label>
            </div>
        </div>
        <div class="div-search-content">
            <input type="text" id="6_txtBDTV_UQ" disabled="disabled" placeholder="@WebApps.Resources.Resource.lblTrang" maxlength="3" style="width:160px" />
        </div>
        @*<div class="div-search-title" style="width:60px">
                <div>@WebApps.Resources.Resource.lblTrang</div>
            </div>*@
    </div>
    <div style="margin-left: 40px;">
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoBanDichTiengViet")
    </div>

    @*<div class="div-search-ad" style="display:inline-flex;margin-bottom:5px">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:1000px">
                    <input type="checkbox" name="ck_TaiLieu_Khac" id="ck_TaiLieu_Khac" onchange="onChangeTLKhac()" />
                    <label>@WebApps.Resources.Resource.lblTaiLieuKhac</label>
                </div>
            </div>
        </div>*@

    <div class="divCover cls_form_title">
        @*<img src="/Content/icons/category.png">*@
        <div class="cls_form_header">@WebApps.Resources.Resource.lblTaiLieuKhac.ToUpper()</div>
        <div style="display: inline-block;  padding-top: 5px; padding-left: 10px;">
            <input type="radio" name="ck_TaiLieu_Khac" id="ck_TaiLieu_Khac" onchange="onChangeTLKhac()">
            <label>@WebApps.Resources.Resource.lblYes</label>
        </div>

        <div style="display: inline-block;padding-left: 5px;">
            <input type="radio" name="ck_TaiLieu_Khac" id="ck_TaiLieu_Khac_No" onchange="onChangeTLKhac()" checked />
            <label>@WebApps.Resources.Resource.lblNo</label>
        </div>
    </div>

    <div id="divTLKhac" class="clssDisplaynone">

        @* Tài liệu xác nhận được phép sử dụng các dấu hiệu đặc biệt (biểu tượng, cờ, huy hiệu, con dấu…), gồm *@
        <div class="div-search-ad" style="display:inline-flex;">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:841px">
                    <input type="checkbox" maxlength="3" value="04NH_D_10" id="6_rdoTLXNDPSD" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                    <label>@WebApps.Resources.Resource.lblTaiLieuXacNhanDuoc</label>
                </div>
            </div>
            <div class="div-search-content">
                <input type="text" id="6_TLXNDPSD" disabled="disabled" placeholder="@WebApps.Resources.Resource.lblTrang" maxlength="3" style="width:160px" />
            </div>
            @*<div class="div-search-title" style="width:60px">
                    <div>@WebApps.Resources.Resource.lblTrang</div>
                </div>*@
        </div>
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoTLXNDPSD")

        @* Tài liệu xác nhận quyền đăng ký nhãn hiệu *@
        <div class="div-search-ad" style="display:inline-flex;">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:1000px">
                    <input type="checkbox" maxlength="3" value="04NH_D_11" id="6_rdoTLDKNH" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                    <label>@WebApps.Resources.Resource.lblTaiLieuXacNhanQuyenDKNH</label>
                </div>
            </div>
        </div>
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoTLDKNH")

        @* Tài liệu xác nhận thụ hưởng quyền đăng ký từ người khác*@
        <div class="div-search-ad" style="display:inline-flex;">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:1000px">
                    <input type="checkbox" maxlength="3" value="04NH_D_12" id="6_rdoTLThuHuongNK" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                    <label>@WebApps.Resources.Resource.lblTaiLieuDKXacNhanThuHuong</label>
                </div>
            </div>
        </div>
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoTLThuHuongNK")

        @* Quy chế sử dụng NH tập thể/chứng nhận, gồm *@
        <div class="div-search-ad" style="display:inherit">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:592px">
                    <input type="checkbox" maxlength="3" value="04NH_D_13" id="6_rdoQuyCheSDNH" name="6_rdoQuyCheSDNH" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                    <label>@WebApps.Resources.Resource.lblQuyCheSuDungNH</label>
                </div>
            </div>
            <div class="div-search-content">
                <div>
                    <input type="text" id="6_txtQuyCheSDNHTrang" disabled="disabled" style="width:160px" maxlength="3" placeholder="@WebApps.Resources.Resource.lblTrang" />
                </div>
            </div>
            <div class="div-search-title" style="width:60px">
                <div>@WebApps.Resources.Resource.lblTrang </div>
            </div>
            <div class="div-search-content">
                <div>
                    <input type="text" id="6_txtQuyCheSDNHBan" style="width:160px" disabled="disabled" maxlength="3" placeholder="@WebApps.Resources.Resource.lblSoBan" />
                </div>
            </div>
            @*<div class="div-search-title" style="width:100px">
                    <div>@WebApps.Resources.Resource.lblBan</div>
                </div>*@
        </div>
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoQuyCheSDNH")

        @* Bản đồ khu vực địa lý *@
        <div class="div-search-ad" style="display:inline-flex;">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:1000px">
                    <input type="checkbox" maxlength="3" value="04NH_D_18" id="6_rdoBanDoKVDL" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                    <label>@WebApps.Resources.Resource.lblBanDoKhuVucDiaLy</label>
                </div>
            </div>
        </div>
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoBanDoKVDL")

        @* Văn bản của UBND tỉnh, TP trực thuộc TW cho phép sử dụng địa danh hoặc dấu hiệu khác chỉ nguồn gốc địa lý của đặc sản địa phượng *@
        <div class="div-search-ad" style="display:inline-flex;">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:1000px">
                    <input type="checkbox" maxlength="3" value="04NH_D_19" id="6_rdoVBuyBanTinh" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                    <label>@WebApps.Resources.Resource.lblVanBanUBNDTinh</label>
                </div>
            </div>
        </div>
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoVBuyBanTinh")

        @* Có tài liệu bổ trợ khai tại trang bổ sung *@
        <div class="div-search-ad" style="display:inline-flex;">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:1000px">
                    <input type="checkbox" maxlength="3" value="04NH_D_20" id="6_rdoTLKhaiTTBS" onchange="return ShowHidenUpLoad(this.id, this.id);" />
                    <label>@WebApps.Resources.Resource.lblCoTaiLieuBoTro</label>
                </div>
            </div>
        </div>
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoTLKhaiTTBS")

        @* Tài liệu khác *@
        <div class="div-search-ad" style="display:inline-flex;">
            <div class="div-search-content">
                <div class="div-checkbox" style="width:1000px">
                    <input type="checkbox" value="04NH_D_22" name="6_rdoTLK" id="6_rdoTLK" />
                    <label>@WebApps.Resources.Resource.lblTaiLieuKhac</label>
                </div>
            </div>
        </div>

    </div>

</div>


<script>

    function onChangeTLKhac() {
        try {
            if ($("#ck_TaiLieu_Khac").is(':checked')) {
                $("#divTLKhac").removeClass("clssDisplaynone");
            } else {
                $("#divTLKhac").addClass("clssDisplaynone");
            }
        } catch (e) {
            console.log(e);
        }
    }

    function funcDetaiBanGoc() {
        try {
            var SoDonTach = document.getElementById("6_txtBangocDNTheoDonSo").value;
            var url = "";
            url = "/trade-mark-share-data/todo-details/" + SoDonTach + "/0";
            $("#hrfDetailBanGoc").attr("href", url);
        } catch (e) {
        }
    }

    $("input[id='6_rdoGiayUyQuyen'").change(function () {
        funcAddRemoveDisabled("6_rdoGiayUyQuyen", "6_txtGiayUQTiengViet");
    });

    $("input[id='6_rdoQuyCheSDNH'").change(function () {
        if ($("#6_rdoQuyCheSDNH").is(':checked')) {
            $("#6_txtQuyCheSDNHTrang").removeAttr('disabled');
            $("#6_txtQuyCheSDNHBan").removeAttr('disabled');
        } else {
            $("#6_txtQuyCheSDNHTrang").attr('disabled', 'disabled');
            $("#6_txtQuyCheSDNHBan").attr('disabled', 'disabled');
        }
    });

    $("input[id='6_rdoTLXNDPSD'").change(function () {
        funcAddRemoveDisabled("6_rdoTLXNDPSD", "6_TLXNDPSD");
    });

    $("input[id='6_rdoBanDichTiengViet'").change(function () {
        funcAddRemoveDisabled("6_rdoBanDichTiengViet", "6_txtBDTV_UQ");
    });


    $("input[id='6_rdoBanGocDaNopTheoDonSo'").change(function () {
        funcAddRemoveDisabled("6_rdoBanGocDaNopTheoDonSo", "6_txtBangocDNTheoDonSo");
    });

    $("input[id='6_rdoBanGocSeNopSau'").change(function () {
        $("#6_txtBangocDNTheoDonSo").attr('disabled', 'disabled');
    });

</script>


