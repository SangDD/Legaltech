        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoBanGoc")
    </div>

    <div class="div-search-ad" style="display:inline-flex;margin-left: 40px;">
        <div class="div-search-content">
            <div class="div-checkbox" style="width:960px">
                <input type="checkbox" id="6_rdoBanSao" value="04NH_D_06" />
