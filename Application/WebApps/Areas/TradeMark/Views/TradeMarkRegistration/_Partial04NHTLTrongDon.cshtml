    <div class="div-search-ad" style="display:inline-flex;margin-left:80px">
        <div class="div-search-content">
            <div class="div-radio" style="width:920px">
                <input type="radio" name="bansao" value="04NH_D_07" id="6_rdoBanGocSeNopSau" onchange="return ShowHidenUpLoadRadioClick(this.id, this.id);" />
