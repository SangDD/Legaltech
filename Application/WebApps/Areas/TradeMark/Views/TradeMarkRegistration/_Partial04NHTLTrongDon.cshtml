    <div style="padding-top: 5px;margin-left: 80px;">
        @Html.Partial("~/Areas/TradeMark/Views/TradeMarkRegistration/_PartialTaiLieuTrongDon.cshtml", "6_rdoBanGocSeNopSau")
    </div>

    <div class="div-search-ad" style="display:inline-flex;margin-left: 80px;">
        <div class="div-search-content">
            <div class="div-radio" style="width:761px">
                <input type="radio" name="bansao" value="04NH_D_08" id="6_rdoBanGocDaNopTheoDonSo" onchange="return ShowHidenUpLoadRadioClick(this.id, this.id);" />
                <label>@WebApps.Resources.Resource.lblBanGocNopTheoDonSo <a href="javascript:;" st class="icon icon-info" style="border:none" data-title="@WebApps.Resources.Resource.lblDatatitle04NHD08"></a></label>
            </div>
