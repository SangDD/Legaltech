    function funGetData(ptype, _UpdateToDo) {
        try {
            //Thông tin chủ đơn
            var formData = new FormData();
            formData.append("pInfo.Id", @_AppHeaderInfo.Id);
            formData.append("pInfo.Case_Code", '@_AppHeaderInfo.Case_Code');

            var reponse = funcThongTinCaseName(formData, "0");
            if (reponse == false) {
                return false;
            }

            if (_UpdateToDo == 1) {
                formData.append("pInfo.UpdateToDo", 1);
            }

            if (ptype == 1) {
                if (funcThongTinChuDon_SaveTem(formData, "1") != true) {
                    return false;
                }
            } else if (ptype == 2) {
                if (funcThongTinChuDon(formData, "1") != true) {
                    return false;
                }
            }

            var v_AppCode = $("#txtAppCode").val();
            if (v_AppCode === "") {
                $('#txtAppCode').focus().val('');
                showError('@Html.Raw(Resource.TM3B_ChonLoaiDonDK)');
                return false;
            }

            // đại diện chủ đơn
            formData.append("pInfo.Appcode", v_AppCode);
            if (funcThongTinDaiDienChuDon(formData, "2") != true) {
                return false;
            }
            var v_RepMasterType = $("input[name='_2_DaiDienChuDon']:checked").val();
            if (ptype == 1) {

            } else if (ptype == 2) {
                if (v_RepMasterType === "" || v_RepMasterType == undefined) {
                    jError("@Html.Raw(Resource.TM2B_ChonDaiDienChuDon)", "@Html.Raw(Resource.TM3B_Loi)", function () {
                        $("#_2_rdoNguoiDaiDienPL").focus()
                    });
                    return false;
                }
            }

            formData.append("pInfo.Rep_Master_Type", v_RepMasterType);
            formData.append("pInfo.Status", ptype);

            //Lấy thông tin tài liệu có trong đơn
            formData.append("pInfo.USE_DOC_OTHERS", 0);
            reponse = funGetDocumentFile(formData);
            if (reponse == false) {
                return false;
            }

            //Tài liệu khác trong đơn
            reponse = funcGetTenTaiLieuKhac(formData);
            if (reponse == false) {
                return false;
            }

         
            //Lấy thông tin detail
            reponse = funcGetAppDetail(formData, ptype);
            if (reponse == false) {
                return false;
            }

            reponse = funcGetCamKetChuDon(formData);
            if (reponse == false) {
                return false;
            }

            $.ajax({
                url: '/trade-mark-c02/do-edit',
                type: 'POST',
                data: formData,
                processData: false,
                contentType: false,
                traditional: true,
                dataType: "json",
                enctype: 'multipart/form-data',
                async: true,
                headers: { "cache-control": "no-cache" },
                beforeSend: function () {
                    CreateRollingWaitingIcon(true);
                },
                success: function (data) {
                    CreateRollingWaitingIcon(false);
                    if (data != null && data.status >= 0) {
                        if (ptype == 1) {
                            jAlert('@Html.Raw(Resource.LuuTamThanhCong)', ThongBao, function () {
                                window.location = "/trade-mark-mana/quan-ly-don";
                            });
                        } else {
                            jAlert('@Html.Raw(Resource.GuiDonThanhCong)', ThongBao, function () {
                                window.location = "/trade-mark-mana/quan-ly-don";
                            });
                        }
                    } else {
                        jError("@Html.Raw(WebApps.Resources.Resource.LoiXuLyDuLieu)", "@Html.Raw(WebApps.Resources.Resource.ThongBao)", function () {
                        });
                    }
                },
                error: function (e) {
                    CreateRollingWaitingIcon(false);
                    console.log(e);
                }
            });
        } catch (e) {
            CreateRollingWaitingIcon(false);
            console.log(e);
        }
    }

    function funcGetAppDetail(formData, pType) {
        try {
            var _type_detail = $("input[name='type_detail']:checked").val();
            if ((_type_detail === "" || _type_detail == undefined) && pType == 2) {
                jError("Chọn Đối tượng yêu cầu duy trì gia hạn", "Lỗi", function () {
                    $("#type_detail_1").focus()
                });
                return false;
            }
            formData.append("pDetail.App_Change_Type", parseInt(_type_detail));

            // số đơn

            if (_type_detail == "1") {

                var app_no_change = $("#App_No_Change_1").val();
                if ((app_no_change === "" || app_no_change == undefined) && pType == 2) {
                    jError("Số văn bằng bảo hộ không được để trống", "@Resource.TM3B_Loi", function () {
                        $("#App_No_Change_1").focus()
                    });
                    return false;
                }

                var app_change_detail = $("#App_Change_Detail_1").val();
                if ((app_change_detail === "" || app_change_detail == undefined) && pType == 2) {
                    jError("Các phương án gia hạn không được để trống", "@Resource.TM3B_Loi", function () {
                        $("#App_Change_Detail_1").focus()
                    });
                    return false;
                }
                formData.append("pDetail.App_No_Change", app_no_change);
                formData.append("pDetail.App_Change_Detail", app_change_detail);
            } else if (_type_detail == "2") {
                var app_no_change = $("#App_No_Change_2").val();
                if ((app_no_change === "" || app_no_change == undefined) && pType == 2) {
                    jError("Số văn bằng bảo hộ không được để trống", "@Resource.TM3B_Loi", function () {
                        $("#App_No_Change_2").focus()
                    });
                    return false;
                }
                var app_change_detail = $("#App_Change_Detail_2").val();
                if ((app_change_detail === "" || app_change_detail == undefined) && pType == 2) {
                    jError("Các phương án gia hạn không được để trống", "@Resource.TM3B_Loi", function () {
                        $("#App_Change_Detail_2").focus()
                    });
                    return false;
                }
                formData.append("pDetail.App_No_Change", app_no_change);
                formData.append("pDetail.App_Change_Detail", app_change_detail);
            } else if (_type_detail == "3") {
                var app_change_detail = $("#App_Change_Detail_3").val();
                if ((app_change_detail === "" || app_change_detail == undefined) && pType == 2) {
                    jError("Số bằng độc quyền không được để trống", "@Resource.TM3B_Loi", function () {
                        $("#App_Change_Detail_3").focus()
                    });
                    return false;
                }
                formData.append("pDetail.App_Change_Detail", app_change_detail);
            } else if (_type_detail == "4") {
                var txtpoint = $("#txtPoint").val();
                if ((txtpoint === "" || txtpoint == undefined) && pType == 2) {
                    jError("Điểm bảo hộ không được để trống", "@Resource.TM3B_Loi", function () {
                        $("#txtPoint").focus()
                    });
                    return false;
                }
                var txtyear = $("#txtYear").val();
                if ((txtyear === "" || txtyear == undefined) && pType == 2) {
                    jError("Năm duy trì hiệu lực không được để trống", "@Resource.TM3B_Loi", function () {
                        $("#txtYear").focus()
                    });
                    return false;
                }
                formData.append("pDetail.Number_Point", txtpoint);
                formData.append("pDetail.Number_Year", txtyear);
            }
        } catch (e) {
            console.log(e);
            showError('@Resource.TM3B_InternetError');
            return false;
        }
    }

